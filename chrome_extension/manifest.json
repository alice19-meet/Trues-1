{
  "manifest_version": 2,

  "name": "Trues",
  "version": "0.1.0",
  "icons": {
    "16": "logo_16.png",
    "48": "logo_48.png",
    "128": "logo_128.png"
  },

  "description": "Trues' app extension",

  "content_scripts": [{
    "css": ["styles.css"],
    "js": ["content.js"],
    "matches": ["https://blog.lateral.io/*"]
  }],
  "browser_action": {

    "default_title": "Google Mail",      
    "default_popup": "poppup.html"       
  }
}
